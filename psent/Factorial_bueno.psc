	count = 0
	Temp = 0
	
	Mientras count < InNum Hacer
		Count1 = 0
		Mientras Count1 < (Count+1) Hacer
			Count1 = Count1 + 1 
			Temp = Temp + temp
			Escribir Count + 1 , " x ", temp 
